public class Escape {
	public static void main(String[] args) {
	
		//add your println statement here
		// I comment my predictions here
		// I said "Hello" to James
		System.out.println("I said \"Hello\" to James.");
		// The string "" is an empty message.
		System.out.println("The string \"\" is an empty message.");
		// Shaq is 7'1
		System.out.println("Shaq is 7'1");
		// \'"\\"
		System.out.println("\\'\"\\\\\"");


	}
}
